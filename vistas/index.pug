html
    head
        title= titulo
        link(rel="stylesheet", href="https://fonts.googleapis.com/icon?family=Material+Icons")
    body 
        div.container
            input(type="text" placeholder="nombre" id="nombre")
            input(type="text" placeholder="numero" id="numero")
            button(id="btn_agregar") agregar
            each agenda in contactos 
                p= `${agenda.id_agenda} -- ${agenda.nombre}`
                span(class="borrar material-icons" id=agenda.id_agenda) delete

    script(src="funciones.js")
